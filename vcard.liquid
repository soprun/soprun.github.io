BEGIN:VCARD
VERSION:4.0
KIND:individual
LANG:{{ site.lang }}
TZ:{{ site.timezone }}
GENDER:{{ site.author.gender }}
NICKNAME;{{ site.encoding }}:{{ site.author.nickname }}

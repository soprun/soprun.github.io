<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {%- if page.noindex == true -%}
        <meta name="robots" content="noindex, nofollow"/>
    {%- endif -%}
    {% assign baseurl = site.baseurl | prepend: site.url | relative_url %}
    <base href="{{ site.env.CF_PAGES_URL | default: baseurl | relative_url }}">

    {% seo %}
    {% feed_meta %}

    <!-- You can set your favicon here -->
    <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.ico' | relative_url }}">

    <link rel="preload" as="style" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.1.3/css/bootstrap.min.css">
    <link rel="preload" as="style" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
    <link rel="preload" as="style"
          href="https://fonts.googleapis.com/css2?family=Noto+Sans:wght@200;300;400;700&display=swap">
    <link rel="preload"
          as="style"
          onload="this.onload=null;this.rel='stylesheet'"
          href="{{ '/assets/css/style.css' | relative_url }}?v={{ site.env.CF_PAGES_COMMIT_SHA }}">
</head>
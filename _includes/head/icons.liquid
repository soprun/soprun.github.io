{%- for item in site.data.links.icons -%}
    {%- assign icons = item.last %}
    {%- assign icons_sizes = icons.sizes | join: ', ' | split: ', ' | sort | join: ', ' | split: ', ' %}
    {%- assign icons_sizes_count = icons.sizes | size %}
    {%- if icons_sizes_count != 0 %}
        {% for size in icons_sizes %}
            {%- assign icons_href = icons.href | append: size %}
            {%- assign icons_size = size | append: 'x' | append: size %}
            {%- include snippet/link.liquid
                params=icons
                href=icons_href
                sizes=icons_size
            %}
        {% endfor %}
    {%- else %}
        {%- include snippet/link.liquid
            params=icons
        %}
    {% endif %}
{%- endfor -%}